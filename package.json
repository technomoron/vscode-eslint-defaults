		"url": "git+ssh://git@github.com/documentmedia/vscode-eslint-defaults.git"
	},
	"license": "ISC",
	"author": "",
	"type": "javascript",
	"private": true,
	"engines": {
		"node": ">=16.0.0"
	},
	"main": "installer.js",
	"scripts": {
