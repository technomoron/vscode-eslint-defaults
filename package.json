{
	"name": "vscode-eslint-defaults",
	"version": "1.0.35",
	"description": "Default editor config and eslint config for js/ts/vue",
	"homepage": "https://github.com/documentmedia/vscode-eslint-defaults#readme",
	"bugs": {
		"url": "https://github.com/documentmedia/vscode-eslint-defaults/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/documentmedia/vscode-eslint-defaults.git"
	},
	"license": "MIT",
	"author": "Technomoron",
	"type": "javascript",
	"engines": {
		"node": ">=22.0.0"
	},
	"main": "installer.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint --no-error-on-unmatched-pattern --ext .js,.cjs,.mjs,.ts,.mts,.tsx,.vue,.md,.json ./ && stylelint --allow-empty-input \"**/*.{css,scss}\"",
		"lintfix": "eslint --fix --no-error-on-unmatched-pattern --ext .js,.cjs,.mjs,.ts,.mts,.tsx,.vue,.md,.json ./ && stylelint --allow-empty-input --fix \"**/*.{css,scss}\"",
		"format": "prettier --write \"**/*.{js,jsx,cjs,mjs,ts,tsx,mts,vue,json,css,scss,md}\"",
		"lintconfig": "node installer/lintconfig.cjs"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"@vue/eslint-config-typescript": "^14.6.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-markdown": "^5.1.0",
		"eslint-plugin-vue": "^10.7.0",
		"jsonc-eslint-parser": "^2.4.2",
		"prettier": "^3.8.1",
		"stylelint": "^17.1.0",
		"stylelint-config-standard-scss": "^17.0.0",
		"vue-eslint-parser": "^10.2.0"
	}
}
